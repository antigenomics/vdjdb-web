{
  "name": "vdjdb-webpack",
  "version": "1.1.1",
  "description": "Webpack configuration for VDJdb-server",
  "private": true,
  "scripts": {
    "start": "node ./webpack/webpack.dev.server",
    "clean:bundle": "rimraf ../../public/bundles/*",
    "build:bundle": "webpack -p --config ./webpack/webpack.prod.config.js",
    "purify:bundle": "purifycss ../../public/bundles/bundle.css ../../public/bundles/bundle.min.js ./views/main.scala.html --min --out ../../public/bundles/bundle.min.css",
    "stats:bundle": "node ./webpack/webpack.prod.postinstall.js",
    "build": "npm run clean:bundle && npm run build:bundle && npm run purify:bundle && npm run stats:bundle",
    "postinstall": "node ./styles/semantic-fix.js"
  },
  "author": "Dmitry Bagaev",
  "homepage": "https://github.com/antigenomics/vdjdb-web-private",
  "dependencies": {
    "@angular/common": "4.3.6",
    "@angular/core": "4.3.6",
    "@angular/forms": "4.3.6",
    "@angular/platform-browser": "4.3.6",
    "@angular/router": "4.3.6",
    "core-js": "2.5.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.4.3",
    "zone.js": "0.8.17"
  },
  "devDependencies": {
    "@angular/compiler": "4.3.6",
    "@angular/compiler-cli": "4.3.6",
    "@angular/platform-browser-dynamic": "4.3.6",
    "@types/angular": "1.6.32",
    "@types/node": "8.0.20",
    "@types/webpack": "3.0.10",
    "@types/uglify-js": "2.6.29",
    "@types/source-map": "0.5.1",
    "@ngtools/webpack": "1.7.1",
    "awesome-typescript-loader": "3.2.3",
    "angular2-template-loader": "0.6.2",
    "raw-loader": "0.5.1",
    "css-loader": "0.28.7",
    "style-loader": "0.18.2",
    "postcss-loader": "2.0.6",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "file-loader": "0.11.2",
    "url-loader": "0.5.9",
    "image-webpack-loader": "3.4.2",
    "extract-text-webpack-plugin": "3.0.0",
    "to-string-loader": "1.1.5",
    "typescript": "2.5.2",
    "tslint": "5.7.0",
    "tslint-webpack-plugin": "1.0.0",
    "codelyzer": "3.2.0",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.8.2",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-merge": "4.1.0",
    "purifycss": "1.2.6",
    "semantic-ui-less": "2.2.12",
    "uglifyjs-webpack-plugin": "0.4.6",
    "rimraf": "2.6.2",
    "console.table": "0.9.1",
    "gzip-size": "4.0.0"
  },
  "engines": {
    "node": ">= 5",
    "npm": ">= 3"
  }
}
