{
  "name": "vdjdb-frontend",
  "version": "2.1.0",
  "description": "Frontend application for VDJdb-server",
  "license": "Apache 2.0",
  "private": true,
  "scripts": {
    "develop:angular": "ng serve -dev --aot --vendor-chunk --watch --port 4200 --host localhost --live-reload=false --delete-output-path=false",
    "develop:webpack": "node ./webpack/webpack.dev.server",

    "bundle": "npm run bundle:aot && npm run bundle:webpack && npm run bundle:stats && npm run bundle:clean:index",
    "bundle:aot": "ng build --aot -prod --extract-licenses --vendor-chunk --output-hashing=none --build-optimizer",
    "bundle:webpack": "webpack -p --config ./webpack/webpack.prod.config.js",
    "bundle:stats": "node ./scripts/stats.prod.js",
    "bundle:clean:index": "rimraf ../../public/bundles/index.html",
    "bundle:clean:public": "rimraf ../../public/bundles/*",
    "lint": "ng lint --type-check --force",

    "help": "ng help",
    "help:serve": "ng help serve",
    "help:build": "ng help build",
    "help:test": "ng help test",
    "help:lint": "ng help lint",
    "help:e2e": "hg help e2e",
    "postinstall": "node ./styles/semantic-fix.js && node ./scripts/purify-patch.install.js",

    "test": "ng test",
    "e2e": "ng e2e"
  },
  "author": "Dmitry Bagaev",
  "homepage": "https://github.com/antigenomics/vdjdb-web-private",
  "dependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.2",
    "zone.js": "0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "rimraf": "2.6.2",
    "console.table": "0.9.1",
    "gzip-size": "4.0.0",
    "raw-loader": "0.5.1",
    "css-loader": "0.28.7",
    "style-loader": "0.19.0",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "file-loader": "1.1.5",
    "url-loader": "0.6.2",
    "image-webpack-loader": "3.4.2",
    "extract-text-webpack-plugin": "3.0.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-merge": "4.1.1",
    "purify-css": "1.2.5",
    "purifycss-webpack": "0.7.0",
    "semantic-ui-less": "2.2.12",
    "glob": "^7.1.2",
    "glob-all": "3.1.0"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  }
}
