{
  "name": "vdjdb-frontend",
  "version": "2.2.2",
  "description": "Frontend application for VDJdb-server",
  "license": "Apache 2.0",
  "private": true,
  "scripts": {
    "develop:angular": "ng serve -dev --aot --vendor-chunk --watch --port 4200 --host localhost --live-reload=false --delete-output-path=false",
    "develop:webpack": "npm run develop:webpack:dll && node ./webpack/webpack.dev.server",
    "develop:webpack:dll": "webpack --config ./webpack/webpack.dll.config.js",
    "bundle": "npm run bundle:aot && npm run bundle:webpack && npm run bundle:stats && npm run bundle:clean:index",
    "bundle:aot": "ng build --aot -prod --extract-licenses --vendor-chunk --output-hashing=none --build-optimizer",
    "bundle:aot:stats": "ng build --aot -prod --extract-licenses --vendor-chunk --output-hashing=none --build-optimizer --stats-json",
    "bundle:webpack": "webpack -p --config ./webpack/webpack.prod.config.js && rimraf ../../public/bundles/styles.css && rimraf ../../public/bundles/styles",
    "bundle:stats": "node ./scripts/stats.prod.js",
    "bundle:clean:index": "rimraf ../../public/bundles/index.html",
    "bundle:clean:public": "rimraf ../../public/bundles/*",
    "lint": "ng lint --type-check --force --format stylish",
    "help": "ng help",
    "help:serve": "ng help serve",
    "help:build": "ng help build",
    "help:test": "ng help test",
    "help:lint": "ng help lint",
    "help:e2e": "hg help e2e",
    "postinstall": "node ./styles/semantic-fix.js && node ./scripts/purify-patch.install.js",
    "test": "ng test",
    "e2e": "ng e2e",
    "clean:node_modules": "rimraf node_modules",
    "clean:cache": "rimraf node_modules/.cache && rimraf webpack/dll"
  },
  "author": "Dmitry Bagaev",
  "homepage": "https://github.com/antigenomics/vdjdb-web-private",
  "dependencies": {
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/router": "5.2.9",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.3",
    "d3": "4.13.0",
    "gzip-js": "0.3.2",
    "rxjs": "5.5.7",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.7.3",
    "@angular/compiler-cli": "5.2.9",
    "@types/d3": "4.13.0",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "9.4.7",
    "angular-router-loader": "0.8.2",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "5.0.0-1",
    "codelyzer": "4.2.1",
    "console.table": "0.10.0",
    "css-loader": "0.28.11",
    "mini-css-extract-plugin": "0.2.0",
    "file-loader": "1.1.11",
    "glob": "7.1.2",
    "glob-all": "3.1.0",
    "gzip-size": "4.1.0",
    "cache-loader": "1.2.2",
    "image-webpack-loader": "4.2.0",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "1.0.0",
    "less": "2.7.3",
    "less-loader": "4.1.0",
    "protractor": "5.3.0",
    "purify-css": "1.2.5",
    "purifycss-webpack": "0.7.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.12",
    "rimraf": "2.6.2",
    "semantic-ui-less": "2.2.14",
    "style-loader": "0.20.3",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typescript": "2.7.2",
    "tsconfig-paths-webpack-plugin": "3.0.2",
    "enhanced-resolve": "4.0.0",
    "url-loader": "1.0.1",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.12",
    "webpack-dev-server": "3.1.1"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "main": "index.js"
}
