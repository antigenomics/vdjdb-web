{
  "name": "vdjdb-frontend",
  "version": "2.1.0",
  "description": "Frontend application for VDJdb-server",
  "license": "Apache 2.0",
  "private": true,
  "scripts": {
    "develop:angular": "ng serve -dev --aot --vendor-chunk --watch --port 4200 --host localhost --live-reload=false --delete-output-path=false",
    "develop:webpack": "node ./webpack/webpack.dev.server",
    "bundle": "npm run bundle:aot && npm run bundle:webpack && npm run bundle:stats && npm run bundle:clean:index",
    "bundle:aot": "ng build --aot -prod --extract-licenses --vendor-chunk --output-hashing=none --build-optimizer",
    "bundle:aot:stats": "ng build --aot -prod --extract-licenses --vendor-chunk --output-hashing=none --build-optimizer --stats-json",
    "bundle:webpack": "webpack -p --config ./webpack/webpack.prod.config.js",
    "bundle:stats": "node ./scripts/stats.prod.js",
    "bundle:clean:index": "rimraf ../../public/bundles/index.html",
    "bundle:clean:public": "rimraf ../../public/bundles/*",
    "lint": "ng lint --type-check --force --format stylish",
    "help": "ng help",
    "help:serve": "ng help serve",
    "help:build": "ng help build",
    "help:test": "ng help test",
    "help:lint": "ng help lint",
    "help:e2e": "hg help e2e",
    "postinstall": "node ./styles/semantic-fix.js && node ./scripts/purify-patch.install.js",
    "test": "ng test",
    "e2e": "ng e2e",
    "clean:node_modules": "rimraf node_modules",
    "clean:cache": "rimraf node_modules/.cache && rimraf webpack/dll"
  },
  "author": "Dmitry Bagaev",
  "homepage": "https://github.com/antigenomics/vdjdb-web-private",
  "dependencies": {
    "@angular/common": "5.1.1",
    "@angular/compiler": "5.1.1",
    "@angular/core": "5.1.1",
    "@angular/forms": "5.1.1",
    "@angular/platform-browser": "5.1.1",
    "@angular/platform-browser-dynamic": "5.1.1",
    "@angular/router": "5.1.1",
    "core-js": "2.5.3",
    "rxjs": "5.5.5",
    "zone.js": "0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "1.6.1",
    "@angular/compiler-cli": "5.1.1",
    "@types/jasmine": "2.8.2",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.5.1",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.4.1",
    "codelyzer": "4.0.2",
    "console.table": "0.9.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "glob": "7.1.2",
    "glob-all": "3.1.0",
    "gzip-size": "4.1.0",
    "hard-source-webpack-plugin": "0.5.5",
    "image-webpack-loader": "3.4.2",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "protractor": "5.2.2",
    "purify-css": "1.2.5",
    "purifycss-webpack": "0.7.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.10",
    "rimraf": "2.6.2",
    "semantic-ui-less": "2.2.12",
    "style-loader": "0.19.1",
    "ts-node": "4.0.1",
    "tslint": "5.8.0",
    "typescript": "2.4.2",
    "uglifyjs-webpack-plugin": "1.1.4",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-merge": "4.1.1"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "main": "index.js"
}
