{
  "name": "vdjdb-webpack",
  "version": "2.1.0",
  "description": "Webpack configuration for VDJdb-server",
  "private": true,
  "scripts": {
    "start": "node ./webpack/webpack.dev.server",
    "bundle": "npm run bundle:clean:all && npm run bundle:aot && npm run bundle:webpack && npm run bundle:stats && npm run bundle:clean:build",
    "bundle:clean:all": "npm run bundle:clean:public && npm run bundle:clean:build",
    "bundle:clean:public": "rimraf ../../public/bundles/*",
    "bundle:aot": "npm run bundle:vendor && npm run bundle:ngc:factory && npm run bundle:ngc:prod && npm run bundle:closure",
    "bundle:vendor": "tsc -p vendor && node ./scripts/rxjs-fix.prod.js",
    "bundle:ngc:factory": "ngc -p tsconfig.ngfactory.json",
    "bundle:ngc:prod": "ngc -p tsconfig.prod.json",
    "bundle:closure": "node ./scripts/build.prod.js",
    "bundle:webpack": "webpack -p --config ./webpack/webpack.prod.config.js",
    "bundle:stats": "node ./scripts/stats.prod.js",
    "bundle:clean:build": "rimraf app/build && rimraf lib",
    "lint": "tslint --project tsconfig.ngfactory.json --config tslint.json --type-check --exclude ./app/main.prod.ts --force ./app/**/*",
    "postinstall": "node ./styles/semantic-fix.js && node ./scripts/rxjs-patch.install.js && node ./scripts/purify-patch.install.js",
    "test": "npm run test:karma",
    "test:karma": "karma start ../../test/frontend/karma.conf.js"
  },
  "author": "Dmitry Bagaev",
  "homepage": "https://github.com/antigenomics/vdjdb-web-private",
  "dependencies": {
    "@angular/common": "4.4.6",
    "@angular/core": "4.4.6",
    "@angular/forms": "4.4.6",
    "@angular/platform-browser": "4.4.6",
    "@angular/router": "4.4.6",
    "core-js": "2.5.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.5.2",
    "zone.js": "0.8.16"
  },
  "devDependencies": {
    "@angular/compiler": "4.4.6",
    "@angular/compiler-cli": "4.4.6",
    "@angular/platform-browser-dynamic": "4.4.6",
    "google-closure-compiler": "^20170409.0.0",
    "glob": "^7.1.2",
    "glob-all": "3.1.0",
    "@types/angular": "1.6.36",
    "@types/node": "8.0.47",
    "awesome-typescript-loader": "3.2.3",
    "angular2-template-loader": "0.6.2",
    "raw-loader": "0.5.1",
    "css-loader": "0.28.7",
    "style-loader": "0.19.0",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "file-loader": "1.1.5",
    "url-loader": "0.6.2",
    "image-webpack-loader": "3.4.2",
    "extract-text-webpack-plugin": "3.0.2",
    "typescript": "2.5.3",
    "tslint": "5.8.0",
    "tslint-webpack-plugin": "1.0.0",
    "codelyzer": "3.2.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-merge": "4.1.0",
    "purify-css": "1.2.5",
    "purifycss-webpack": "0.7.0",
    "semantic-ui-less": "2.2.12",
    "uglifyjs-webpack-plugin": "1.0.1",
    "rimraf": "2.6.2",
    "console.table": "0.9.1",
    "gzip-size": "4.0.0",
    "md5-file": "3.2.3",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-ie-launcher": "1.0.0",
    "karma-safari-launcher": "1.0.0",
    "karma-opera-launcher": "1.0.0",
    "karma-firefox-launcher": "1.0.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  }
}
