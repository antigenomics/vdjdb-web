{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "vdjdb-frontend"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "../../public/bundles",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test/frontend/src/test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../test/frontend/src/tsconfig.spec.json",
      "prefix": "app",
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": { "config": "../../test/frontend/protractor.conf.js" }
  },
  "lint": [
    { "project": "src/tsconfig.app.json", "exclude": [ "**/node_modules/**" , "**/polyfills.ts" ] }
  ],
  "test": {
    "karma": { "config": "../../test/frontend/karma.conf.js" }
  },
  "defaults": { "styleExt": "css", "component": {} }
}
