{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2015",
    "module": "es2015",
    "sourceMap": true,
    "lib": [ "dom", "es2015" ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "typeRoots": [ "../../app/frontend/node_modules/@types" ],
    "types": [ "node" ],
    "noImplicitAny": true,
    "removeComments": true,
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "allowJs": true
  },
  "exclude": [ ],
  "compileOnSave": false,
  "buildOnSave": false
}